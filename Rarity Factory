local key

function CheckKey()
   if key == "TyToExist" then -- Bro you are insane! You found the key by searching here.. ( Pro Security )
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Saaakai/Random-Roblox-Script/main/Rarity%20Factory%20Script.lua"))()
   end
end

